      </ImageView>
      <JFXTextField fx:id="name" layoutX="188.0" layoutY="114.0" promptText="Name:" />
      <JFXTextField fx:id="emai" layoutX="188.0" layoutY="199.0" promptText="Email" />
      <JFXPasswordField fx:id="pass" layoutX="188.0" layoutY="270.0" promptText="Password" />
